{
  "include": ["./src/"],
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": false,
    "alwaysStrict": true,
    "pretty": false,
    "outDir": "dist",
    "module": "commonjs",
    "target": "ES6",
    "sourceMap": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
